LOAD "testlib.lgo

TO BUTLASTTEST

   SIMPLEREPORTTEST [BL      [1 2 3] ] [1 2]
   SIMPLEREPORTTEST [BUTLAST [1 2 3] ] [1 2]
   SIMPLEREPORTTEST [BUTLAST [1]     ] []
   SIMPLEREPORTTEST [BUTLAST "hello  ] "hell
   SIMPLEREPORTTEST [BUTLAST "h      ] "||

   ; bad number of inputs
   RUNNOTENOUGHINPUTSTEST [(BUTLAST)]
   RUNTOOMANYINPUTSTEST   [(BUTLAST "word "toomany)]

   ; bad inputs
   RUNBADINPUTTEST [BUTLAST []  ]
   RUNBADINPUTTEST [BUTLAST "|| ]
   RUNBADINPUTTEST [BUTLAST {1} ]

END
