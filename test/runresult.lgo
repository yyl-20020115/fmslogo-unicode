LOAD "testlib.lgo

TO RUNRESULTTEST.BADINPUT

   RUNNOTENOUGHINPUTSTEST [(RUNRESULT)]
   RUNTOOMANYINPUTSTEST   [(RUNRESULT "SUM "toomany)]

   RUNBADINPUTTEST       [ RUNRESULT {1 2 3}        ] ; bug #200
   RUNIDONTKNOWHOWTOTEST [ RUNRESULT DOES.NOT.EXIST ]

   RUNYOUDONTSAYWHATTODOWITHTEST [ RUNRESULT [1 2 3]  ]

END

TO RUNRESULTTEST.SMOKETEST

   ; from the manual
   SIMPLEREPORTTEST [ RUNRESULT [FIRST [1 2 3]] ] [1]
   SIMPLEREPORTTEST [ RUNRESULT [PENDOWN]       ] []

   ; strange, but legal, input
   SIMPLEREPORTTEST [ RUNRESULT []       ] []
   SIMPLEREPORTTEST [ RUNRESULT 100      ] [100]
   SIMPLEREPORTTEST [ RUNRESULT [100]    ] [100]

END

TO RUNRESULTTEST

   RUNRESULTTEST.SMOKETEST
   RUNRESULTTEST.BADINPUT

END

