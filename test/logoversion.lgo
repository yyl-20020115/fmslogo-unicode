LOAD "testlib.lgo

TO LOGOVERSIONTEST.BADINPUT
   RUNTOOMANYINPUTSTEST [(LOGOVERSION "toomany)]
END

TO LOGOVERSIONTEST.SMOKETEST
   LOCAL [version]

   TRACEINSTRUCTION [MAKE "version LOGOVERSION]
   FAILIFNOTTRUE    [LIST? :version]
   SIMPLEREPORTTEST [COUNT :version]  4
   SIMPLEREPORTTEST [ITEM 1 :version] "FMSLogo
   SIMPLEREPORTTEST [ITEM 2 :version] "|7.3.0|
   SIMPLEREPORTTEST [ITEM 3 :version] "Windows
   SIMPLEREPORTTEST [ITEM 4 :version] 1033

END

TO LOGOVERSIONTEST

   LOGOVERSIONTEST.SMOKETEST
   LOGOVERSIONTEST.BADINPUT

END
