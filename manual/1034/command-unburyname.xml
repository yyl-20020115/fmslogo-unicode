<indexterm><primary>UNBURYNAME</primary></indexterm>

<variablelist>

<varlistentry><term>Sinopsis</term>
<listitem>
<synopsis><command>DESTAPANOMBRE</command> <parameter>nombre_de_variable</parameter>
