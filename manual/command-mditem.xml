<listitem><simplelist>
<member><link linkend="command-mdarray">MDARRAY</link></member>
<member><link linkend="command-mdsetitem">MDSETITEM</link></member>
<member><link linkend="command-item">ITEM</link></member>
</simplelist></listitem>
</varlistentry>


</variablelist>
</section>
